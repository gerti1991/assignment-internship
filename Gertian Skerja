array_a = [1, 2, 3, 8, 100, 4, 65, 9, 7]

array_b = [5, 6, 43, 8, 39, 74, 9, 100]


def merge(a, b):
    count =0
    union = a
    for e in b:
        for e2 in a:
            if e2 == e:
                count+=1
        if count == 0:
            union+=[e]
    count =0
       
    return union


print(merge(array_a, array_b))

